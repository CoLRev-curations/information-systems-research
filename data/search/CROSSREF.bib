   doi                           = {10.1287/ISRE.4.1.88},
   author                        = {Bretschneider, Stuart and Wittmer, Dennis},
   journal                       = {Information Systems Research},
   title                         = {Organizational Adoption of Microcomputer Technology - The Role of Sector},
   year                          = {1993},
   volume                        = {4},
   number                        = {1},
   pages                         = {88--108},
