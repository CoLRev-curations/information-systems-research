   author                        = {Vaast, Emmanuelle and Walsham, Geoff},
   journal                       = {Information Systems Research},
   title                         = {Trans-Situated Learning - Supporting a Network of Practice with an Information Infrastructure},
   year                          = {2009},
   volume                        = {20},
   number                        = {4},
   pages                         = {547--564},
