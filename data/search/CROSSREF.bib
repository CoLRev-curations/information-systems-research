   title                         = {The Effects of Operational and Financial Performance Failure on BI&A-Enabled Search Behaviors: A Theory of Performance-Driven Search},
   year                          = {2020},
   volume                        = {31},
   number                        = {4},
   pages                         = {1144--1163},
   abstract                      = {Abstract for Practice and Policy},
   cited_by                      = {8},
   language                      = {eng},
