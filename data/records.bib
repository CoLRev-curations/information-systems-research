@article{FichmanKohliKrishnan2011,
   colrev_origin                 = {CROSSREF.bib/000183},
   colrev_status                 = {md_prepared},
   colrev_masterdata_provenance  = {journal:http://dx.doi.org/10.1287/ISRE.1110.0382;;
                                    title:http://dx.doi.org/10.1287/ISRE.1110.0382;;
                                    year:LocalIndexPrep;;
                                    volume:http://dx.doi.org/10.1287/ISRE.1110.0382;;
                                    number:http://dx.doi.org/10.1287/ISRE.1110.0382;;
                                    author:LOADER.import_provenance;;},
   colrev_data_provenance        = {doi:https://api.crossref.org/works/10.1287/ISRE.1110.0382;;
                                    abstract:https://api.crossref.org/works/10.1287/ISRE.1110.0382;;
                                    language:https://api.crossref.org/works/10.1287/ISRE.1110.0382;;
                                    cited_by:http://dx.doi.org/10.1287/ISRE.1110.0382;;
                                    url:https://www.doi.org/10.1287/ISRE.1110.0382;;
                                    colrev_masterdata_provenance:man_prep;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|22|3|2011|fichman-kohli-krishnan|editorial-overviewthe-role-of-information-systems-in-healthcare-current-research-and-future-trends;},
   doi                           = {10.1287/ISRE.1110.0382},
   author                        = {Fichman, Robert G. and Kohli, Rajiv and Krishnan, Ranjani},
   journal                       = {Information Systems Research},
   title                         = {Editorial Overview—The Role of Information Systems in Healthcare: Current Research and Future Trends},
   year                          = {2011},
   volume                        = {22},
   number                        = {3},
   abstract                      = {Information systems have great potential to reduce healthcare costs and improve outcomes. The purpose of this special issue is to offer a forum for theory-driven research that explores the role of IS in the delivery of healthcare in its diverse organizational and regulatory settings. We identify six theoretically distinctive elements of the healthcare context and discuss how these elements increase the motivation for, and the salience of, the research results reported in the nine papers comprising this special issue. We also provide recommendations for future IS research focusing on the implications of technology-driven advances in three areas: social media, evidence-based medicine, and personalized medicine.},
   cited_by                      = {227},
   language                      = {en},
   url                           = {https://pubsonline.informs.org/doi/10.1287/isre.1110.0382},
}
