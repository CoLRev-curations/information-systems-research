@article{JohnsonSafadiFaraj2015,
   colrev_origin                 = {CROSSREF.bib/JohnsonSafadiFaraj2015;DBLP.bib/000780;pdfs.bib/0000000331},
   colrev_status                 = {pdf_imported},
