   colrev_id                     = {colrev_id1:|a|information-systems-research|21|4|2010|agarwal-gao-desroches-jha|research-commentary-the-digital-transformation-of-healthcare-current-status-and-the-road-ahead;
                                    colrev_id1:|a|information-systems-research|21|4|2010|agarwal-gao-desroches-jha|research-commentarythe-digital-transformation-of-healthcare-current-status-and-the-road-ahead;},
   colrev_pdf_id                 = {cpid1:ffffffffffffffffe03fffc7e01fff87fffffffffff00ffffc00003ffc00007ffff2a7ffff0000ffff0000ffffe007ffff0400ffff0000fff000000ff000000ff000000ff000000ff005801ff000001ffa0050fff0ffa407e000800fe0008007e0010007e0000007e0000007e0010007e0008007ffffffffffffffff8bffffff},
