   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/15_4/RaghuJayaramanRao2004.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.1040.0031;;
                                    language:http://dx.doi.org/10.1287/ISRE.1040.0031;;
