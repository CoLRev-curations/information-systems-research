   colrev_id                     = {colrev_id1:|a|information-systems-research|21|4|2010|tiwana-konsynski-bush|research-commentary-platform-evolution-coevolution-of-platform-architecture-governance-and-environmental-dynamics;
                                    colrev_id1:|a|information-systems-research|21|4|2010|tiwana-konsynski-bush|research-commentaryplatform-evolution-coevolution-of-platform-architecture-governance-and-environmental-dynamics;},
