@article{SenRaghu2013,
   colrev_origin                 = {CROSSREF.bib/SenRaghu2013;DBLP.bib/000656;pdfs.bib/0000000783},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/24_3/SenRaghu2013.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2013.0475;;
                                    language:http://dx.doi.org/10.1287/ISRE.2013.0475;;
                                    cited_by:http://dx.doi.org/10.1287/ISRE.2013.0475;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|24|3|2013|sen-raghu|interdependencies-in-it-infrastructure-services-analyzing-service-processes-for-optimal-incentive-design;},
   file                          = {pdfs/24_3/SenRaghu2013.pdf},
   doi                           = {10.1287/ISRE.2013.0475},
   dblp_key                      = {https://dblp.org/rec/journals/isr/SenR13},
   author                        = {Sen, Sagnika and Raghu, T. S.},
   journal                       = {Information Systems Research},
   title                         = {Interdependencies in IT Infrastructure Services - Analyzing Service Processes for Optimal Incentive Design},
   year                          = {2013},
   volume                        = {24},
   number                        = {3},
   pages                         = {822--841},
   url                           = {https://www.doi.org/10.1287/ISRE.2013.0475},
   abstract                      = {Information technology (IT) infrastructure outsourcing arrangements involve multiple services and processes that are interdependent. The interdependencies pose significant challenges in designing appropriate incentives to influence a provider's effort-allocation decisions. By integrating process modeling fundamentals with multitask agency theory, we enumerate the base set of possible interrelationships among different IT service processes and derive corresponding optimal incentives. Our results demonstrate the impacts of risk profile, random noise, value-cost ratio, and process structure on optimal incentive rates. We find that the current practice of treating IT services as essentially independent is optimal only in limited settings where both the service provider and customer are risk neutral. Interestingly, incongruent performance measures require optimal incentive rates to respond in complex ways to the strength of coupling between services and the complementarity and substitutability of services. We also analyze more complex process scenarios using different combinations of the base set. The results demonstrate that, while the findings from the base set largely hold, the value-cost ratio of the services and the performance measure congruity can pose unique challenges in determining incentive rates.},
   language                      = {en},
   cited_by                      = {9},
}

@article{SenRaghuVinze2009,
   colrev_origin                 = {CROSSREF.bib/SenRaghuVinze2009;DBLP.bib/000440;pdfs.bib/0000000822},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/20_2/SenRaghuVinze2009.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.1080.0196;;
                                    language:http://dx.doi.org/10.1287/ISRE.1080.0196;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.1080.0196;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|20|2|2009|sen-raghu-vinze|demand-heterogeneity-in-it-infrastructure-services-modeling-and-evaluation-of-a-dynamic-approach-to-defining-service-levels;},
   file                          = {pdfs/20_2/SenRaghuVinze2009.pdf},
   doi                           = {10.1287/ISRE.1080.0196},
   dblp_key                      = {https://dblp.org/rec/journals/isr/SenRV09},
   author                        = {Sen, Sagnika and Raghu, T. S. and Vinze, Ajay S.},
   journal                       = {Information Systems Research},
   title                         = {Demand Heterogeneity in IT Infrastructure Services - Modeling and Evaluation of a Dynamic Approach to Defining Service Levels},
   year                          = {2009},
   volume                        = {20},
   number                        = {2},
   pages                         = {258--276},
   url                           = {https://www.doi.org/10.1287/ISRE.1080.0196},
   abstract                      = {A key feature of service-oriented models of information technology is the promise of prespecified quality levels enforceable via service level agreements (SLAs). This poses difficult management problems when considerable variability exists in user preferences and service demand within any organization. Because variance in expectations impact service levels, effective pricing and resource allocation mechanisms are needed to deliver services at the promised quality level. In this paper, we propose a mechanism for SLA formulation that is responsive to demand fluctuations and user preference variance, with the objective of maximizing organizational welfare of the participants. This formulation features a dynamic priority based price-penalty scheme targeted to individual users. An analytical model is presented and evaluated for effectiveness of a proposed dynamic priority-based pricing scheme vis-à-vis a baseline fixed-price single-quality level SLA. Simulations using data from an existing SLA is used to provide evidence that the proposed dynamic pricing scheme is likely to be more effective than a fixed-price approach from a system welfare perspective.},
   language                      = {en},
   cited_by                      = {20},
}

