@article{GuttHerrmannRahman2019,
   colrev_origin                 = {CROSSREF.bib/GuttHerrmannRahman2019;DBLP.bib/000995;pdfs.bib/0000000036},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/30_3/GuttHerrmannRahman2019.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2019.0845;;
