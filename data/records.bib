   colrev_id                     = {colrev_id1:|a|information-systems-research|24|1|2013|aral-dellarocas-godes|introduction-to-the-special-issue-social-media-and-business-transformation-a-framework-for-research;
                                    colrev_id1:|a|information-systems-research|24|1|2013|aral-dellarocas-godes|introduction-to-the-special-issuesocial-media-and-business-transformation-a-framework-for-research;},
