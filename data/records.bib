   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/18_2/ChenSeshadri2007.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.1070.0119;;
