                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.2014.0543;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|25|4|2014|rigdon-becker-rai-ringle-diamantopoulos-karahanna-straub-dijkstra|conflating-antecedents-and-formative-indicators-a-comment-on-aguirre-urreta-and-marakas;},
