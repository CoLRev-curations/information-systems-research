   colrev_id                     = {colrev_id1:|a|information-systems-research|24|2|2013|gupta-bostrom|an-investigation-of-the-appropriation-of-technology-mediated-training-methods-incorporating-enactive-and-collaborative-learning;
                                    colrev_id1:|a|information-systems-research|24|2|2013|gupta-bostrom|research-notean-investigation-of-the-appropriation-of-technology-mediated-training-methods-incorporating-enactive-and-collaborative-learning;},
