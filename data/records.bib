}

@article{EasleyGuoKramer2018,
   colrev_origin                 = {CROSSREF.bib/EasleyGuoKramer2018;DBLP.bib/000900;pdfs.bib/0000000689},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/29_2/EasleyGuoKramer2018.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2017.0740;;
                                    language:http://dx.doi.org/10.1287/ISRE.2017.0740;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.2017.0740;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|29|2|2018|easley-guo-kramer|research-commentary-from-net-neutrality-to-data-neutrality-a-techno-economic-framework-and-research-agenda;
                                    colrev_id1:|a|information-systems-research|29|2|2018|easley-guo-kramer|research-commentaryfrom-net-neutrality-to-data-neutrality-a-techno-economic-framework-and-research-agenda;},
