                                    volume:https://api.crossref.org/works/10.1287/ISRE.2021.1074;;
                                    number:https://api.crossref.org/works/10.1287/ISRE.2021.1074;;
                                    author:https://api.crossref.org/works/10.1287/ISRE.2021.1074;;},
   colrev_data_provenance        = {doi:https://api.crossref.org/works/10.1287/ISRE.2021.1074;;
