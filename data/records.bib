@article{TodriGhoseSingh2020,
   colrev_origin                 = {CROSSREF.bib/TodriGhoseSingh2020;DBLP.bib/001165;pdfs.bib/0000000640},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/31_1/TodriGhoseSingh2020.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2019.0877;;
                                    language:http://dx.doi.org/10.1287/ISRE.2019.0877;;
