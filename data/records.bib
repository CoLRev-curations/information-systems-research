@article{PuLiuChenEtAl2022,
   colrev_origin                 = {CROSSREF.bib/000172},
   colrev_status                 = {md_prepared},
   colrev_masterdata_provenance  = {journal:https://api.crossref.org/works/10.1287/ISRE.2021.1052;;
