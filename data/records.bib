@article{CampbellWellsValacich2013,
   colrev_origin                 = {CROSSREF.bib/CampbellWellsValacich2013;DBLP.bib/000667;pdfs.bib/0000000718},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/24_2/CampbellWellsValacich2013.pdf;;
