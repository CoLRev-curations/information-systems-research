@article{Unknown2012a,
   colrev_origin                 = {CROSSREF.bib/000070},
   colrev_status                 = {rev_prescreen_excluded},
   colrev_masterdata_provenance  = {journal:https://api.crossref.org/works/10.1287/ISRE.1120.0459;;
                                    title:https://api.crossref.org/works/10.1287/ISRE.1120.0459;;
                                    year:https://api.crossref.org/works/10.1287/ISRE.1120.0459;;
                                    volume:https://api.crossref.org/works/10.1287/ISRE.1120.0459;;
                                    number:https://api.crossref.org/works/10.1287/ISRE.1120.0459;;},
   colrev_data_provenance        = {doi:https://api.crossref.org/works/10.1287/ISRE.1120.0459;;
                                    abstract:https://api.crossref.org/works/10.1287/ISRE.1120.0459;;
                                    language:https://api.crossref.org/works/10.1287/ISRE.1120.0459;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.1120.0459;;
                                    url:https://api.crossref.org/works/10.1287/ISRE.1120.0459;;},
   prescreen_exclusion           = {complementary material},
   doi                           = {10.1287/ISRE.1120.0459},
