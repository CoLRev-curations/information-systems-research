   colrev_id                     = {colrev_id1:|a|information-systems-research|19|1|2008|devaraj-easley-crant|research-note-how-does-personality-matter-relating-the-five-factor-model-to-technology-acceptance-and-use;
                                    colrev_id1:|a|information-systems-research|19|1|2008|devaraj-easley-crant|research-notehow-does-personality-matter-relating-the-five-factor-model-to-technology-acceptance-and-use;},
