@article{ChwelosRamirezKraemerEtAl2010,
   colrev_origin                 = {CROSSREF.bib/ChwelosRamirezKraemerEtAl2010;DBLP.bib/000462;pdfs.bib/0000000760},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/21_2/ChwelosRamirezKraemerEtAl2010.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.1090.0229;;
                                    language:http://dx.doi.org/10.1287/ISRE.1090.0229;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.1090.0229;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|21|2|2010|chwelos-ramirez-kraemer-melville|research-note-does-technological-progress-alter-the-nature-of-information-technology-as-a-production-input-new-evidence-and-new-results;
                                    colrev_id1:|a|information-systems-research|21|2|2010|chwelos-ramirez-kraemer-melville|research-notedoes-technological-progress-alter-the-nature-of-information-technology-as-a-production-input-new-evidence-and-new-results;},
   file                          = {pdfs/21_2/ChwelosRamirezKraemerEtAl2010.pdf},
   doi                           = {10.1287/ISRE.1090.0229},
   dblp_key                      = {https://dblp.org/rec/journals/isr/ChwelosRKM10},
   author                        = {Chwelos, Paul and Ramirez, Ronald and Kraemer, Kenneth L. and Melville, Nigel},
   journal                       = {Information Systems Research},
   title                         = {Research Note - Does Technological Progress Alter the Nature of Information Technology as a Production Input? New Evidence and New Results},
   year                          = {2010},
   volume                        = {21},
   number                        = {2},
   pages                         = {392--408},
   url                           = {https://www.doi.org/10.1287/ISRE.1090.0229},
   abstract                      = {Prior research at the firm level finds information technology (IT) to be a net substitute for both labor and non-IT capital inputs. However, it is unclear whether these results hold, given recent IT innovations and continued price declines. In this study we extend prior research to examine whether these input relationships have evolved over time. First, we introduce new price indexes to account for varying technological progress across different types of IT hardware. Second, we use the rental price methodology to measure capital in terms of the flow of services provided. Finally, we use hedonic methods to extend our IT measures to 1998, enabling analysis spanning the emergence of the Internet. Analyzing approximately 9,800 observations from over 800 Fortune 1,000 firms for the years 1987–1998, we find firm demand for IT to be elastic for decentralized IT and inelastic for centralized IT. Moreover, Allen Elasticity of Substitution estimates confirm that through labor substitution, the increasing factor share of IT comes at the expense of labor. Last, we identify a complementary relationship between IT and ordinary capital, suggesting an evolution in this relationship as firms have shifted to more decentralized organizational forms. We discuss these results in terms of prior research, suggest areas of future research, and discuss managerial implications. *This paper is dedicated to the memory of Paul Chwelos, respected colleague and dear friend.},
   language                      = {en},
   cited_by                      = {51},
}

@article{ClarkeBurtonJonesWeber2016,
   colrev_origin                 = {CROSSREF.bib/ClarkeBurtonJonesWeber2016;DBLP.bib/000827;pdfs.bib/0000001092},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/27_2/ClarkeBurtonJonesWeber2016.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2016.0631;;
                                    language:http://dx.doi.org/10.1287/ISRE.2016.0631;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.2016.0631;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|27|2|2016|clarke-burton-jones-weber|on-the-ontological-quality-and-logical-quality-of-conceptual-modeling-grammars-the-need-for-a-dual-perspective;},
   file                          = {pdfs/27_2/ClarkeBurtonJonesWeber2016.pdf},
   doi                           = {10.1287/ISRE.2016.0631},
   dblp_key                      = {https://dblp.org/rec/journals/isr/ClarkeBW16},
   author                        = {Clarke, Roger and Burton-Jones, Andrew and Weber, Ron},
   journal                       = {Information Systems Research},
   title                         = {On the Ontological Quality and Logical Quality of Conceptual-Modeling Grammars - The Need for a Dual Perspective},
   year                          = {2016},
   volume                        = {27},
   number                        = {2},
   pages                         = {365--382},
   url                           = {https://www.doi.org/10.1287/ISRE.2016.0631},
   abstract                      = {A core activity in information systems development involves building a conceptual model of the domain that an information system is intended to support. Such models are created using a conceptual-modeling (CM) grammar. Just as high-quality conceptual models facilitate high-quality systems development, high-quality CM grammars facilitate high-quality CM. This paper provides a new perspective on ways to improve the quality of the semantics of CM grammars. For many years, the leading approach to this topic has relied on ontological theory. We show, however, that the ontological approach captures only half the story; it needs to be coupled with a logical approach. We explain how the ontological and logical qualities of CM grammars interrelate. Furthermore, we outline three contributions of a logical approach to evaluating the quality of CM grammars: a means of seeing some familiar CM problems in simpler ways, illumination of new problems, and proving the benefit of modifying existing CM grammars in particular ways. We demonstrate these benefits in the context of the Entity-Relationship grammar. More generally, our paper opens a new area of research with many opportunities for future research and practice.},
   language                      = {en},
   cited_by                      = {9},
}

@article{ClarkeChenDuEtAl2021,
   colrev_origin                 = {CROSSREF.bib/ClarkeChenDuEtAl2021;DBLP.bib/001069;pdfs.bib/0000000018},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/32_1/ClarkeChenDuEtAl2021.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2019.0910;;
                                    language:http://dx.doi.org/10.1287/ISRE.2019.0910;;
                                    cited_by:http://dx.doi.org/10.1287/ISRE.2019.0910;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|32|1|2021|clarke-chen-du-hu|fake-news-investor-attention-and-market-reaction;},
   file                          = {pdfs/32_1/ClarkeChenDuEtAl2021.pdf},
   doi                           = {10.1287/ISRE.2019.0910},
   dblp_key                      = {https://dblp.org/rec/journals/isr/ClarkeCDH21},
   author                        = {Clarke, Jonathan and Chen, Hailiang and Du, Ding and Hu, Yu},
   journal                       = {Information Systems Research},
   title                         = {Fake News, Investor Attention, and Market Reaction},
   year                          = {2021},
   volume                        = {32},
   number                        = {1},
   pages                         = {35--52},
   url                           = {https://www.doi.org/10.1287/ISRE.2019.0910},
   abstract                      = {Does fake news in financial markets attract more investor attention and have a significant impact on stock prices? The authors use the SEC crackdown of stock promotion schemes in April 2017 to examine investor attention and the stock price reaction to fake news articles. Using data from Seeking Alpha, the authors find that fake news stories generate significantly more attention than a control sample of legitimate articles. The authors find no evidence that article commenters can detect fake news, and they find that Seeking Alpha editors have only modest ability to detect fake news. However, the authors implement several well-known machine learning algorithms based on linguistic characteristics and show that machine learning algorithms can successfully identify fake news. In addition, the stock market appears to price fake news correctly. While abnormal trading volume increases around the release of fake news, the increase is less than that observed for legitimate news. The stock price reaction to fake news is discounted when compared with legitimate news articles.},
   language                      = {en},
   cited_by                      = {20},
}

@article{ClaussenKretschmerMayrhofer2013,
   colrev_origin                 = {CROSSREF.bib/ClaussenKretschmerMayrhofer2013;DBLP.bib/000670;pdfs.bib/0000001139},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/24_1/ClaussenKretschmerMayrhofer2013.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.1120.0467;;
                                    language:http://dx.doi.org/10.1287/ISRE.1120.0467;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.1120.0467;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|24|1|2013|claussen-kretschmer-mayrhofer|the-effects-of-rewarding-user-engagement-the-case-of-facebook-apps;},
   file                          = {pdfs/24_1/ClaussenKretschmerMayrhofer2013.pdf},
   doi                           = {10.1287/ISRE.1120.0467},
   dblp_key                      = {https://dblp.org/rec/journals/isr/ClaussenKM13},
   author                        = {Claussen, Jörg and Kretschmer, Tobias and Mayrhofer, Philip},
   journal                       = {Information Systems Research},
   title                         = {The Effects of Rewarding User Engagement - The Case of Facebook Apps},
   year                          = {2013},
   volume                        = {24},
   number                        = {1},
   pages                         = {186--200},
   url                           = {https://www.doi.org/10.1287/ISRE.1120.0467},
   abstract                      = {We study the market for apps on Facebook, the dominant social networking platform, and make use of a rule change by Facebook by which highly engaging apps were rewarded with further opportunities to engage users. The rule change led to new applications with significantly higher user ratings being developed. Moreover, user ratings became more important drivers of app success. Other drivers of app success are also affected by the rule change; sheer network size became a less important driver for app success, update frequency benefitted apps more in staying successful, and active users of Facebook apps declined less rapidly with age. Our results show that social media channels do not necessarily have to be managed through hard exclusion of participants but can also be steered through “softer” changes in reward and incentive systems.},
   language                      = {en},
   cited_by                      = {102},
