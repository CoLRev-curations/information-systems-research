   colrev_id                     = {colrev_id1:|a|information-systems-research|26|4|2015|choudhary-zhang|research-note-patching-the-cloud-the-impact-of-saas-on-patching-strategy-and-the-timing-of-software-release;
                                    colrev_id1:|a|information-systems-research|26|4|2015|choudhary-zhang|research-notepatching-the-cloud-the-impact-of-saas-on-patching-strategy-and-the-timing-of-software-release;},
