@article{GleasureConboyMorgan2019,
   colrev_origin                 = {CROSSREF.bib/GleasureConboyMorgan2019;DBLP.bib/000962;pdfs.bib/0000000643},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/30_2/GleasureConboyMorgan2019.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2019.0840;;
                                    language:http://dx.doi.org/10.1287/ISRE.2019.0840;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.2019.0840;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|30|2|2019|gleasure-conboy-morgan|talking-up-a-storm-how-backers-use-public-discourse-to-exert-control-in-crowdfunded-systems-development-projects;},
   file                          = {pdfs/30_2/GleasureConboyMorgan2019.pdf},
   doi                           = {10.1287/ISRE.2019.0840},
   dblp_key                      = {https://dblp.org/rec/journals/isr/GleasureCM19},
   author                        = {Gleasure, Rob and Conboy, Kieran and Morgan, Lorraine},
   journal                       = {Information Systems Research},
   title                         = {Talking Up a Storm - How Backers Use Public Discourse to Exert Control in Crowdfunded Systems Development Projects},
   year                          = {2019},
   volume                        = {30},
   number                        = {2},
   pages                         = {447--465},
   url                           = {https://www.doi.org/10.1287/ISRE.2019.0840},
   abstract                      = {New models of information systems development (ISD) have emerged to accommodate distributed participation and large numbers of contributors. Crowdfunding extends distributed models by allowing members of the public to finance a project, bypassing or lessening the need for traditional investor-controllers. Backers lack most defined power relationships and control mechanisms available to traditional investor-controllers. Yet some backers still successfully assume the power to influence the projects they support. This challenges traditional notions of power and control in ISD. We thus study two famous crowdfunded cases to explore how power and control are attained in these projects. Findings show that backers do indeed collectively impose each of the same high-level control types as traditional investor-controllers, albeit the specific controls imposed may vary. Key to backers’ power is their ability to construct a positive or negative public image around a project, a power for which they continuously battle among themselves. This uncovers important vulnerabilities in crowdfunding, many of which emerge when public perceptions of project failure are high. Under these conditions, backers may become increasingly assertive, which paradoxically causes owners to become less compliant. This study further identifies issues with attracting large numbers of backers, as power and control dynamics become increasingly unstable.},
   language                      = {en},
   cited_by                      = {11},
}

@article{GnyawaliFanPenner2010,
   colrev_origin                 = {CROSSREF.bib/GnyawaliFanPenner2010;DBLP.bib/000499;pdfs.bib/0000001107},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/21_3/GnyawaliFanPenner2010.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.1100.0294;;
                                    language:http://dx.doi.org/10.1287/ISRE.1100.0294;;
                                    cited_by:http://dx.doi.org/10.1287/ISRE.1100.0294;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|21|3|2010|gnyawali-fan-penner|competitive-actions-and-dynamics-in-the-digital-age-an-empirical-investigation-of-social-networking-firms;},
   file                          = {pdfs/21_3/GnyawaliFanPenner2010.pdf},
   doi                           = {10.1287/ISRE.1100.0294},
   dblp_key                      = {https://dblp.org/rec/journals/isr/GnyawaliFP10},
   author                        = {Gnyawali, Devi R. and Fan, Weiguo and Penner, James},
   journal                       = {Information Systems Research},
   title                         = {Competitive Actions and Dynamics in the Digital Age - An Empirical Investigation of Social Networking Firms},
   year                          = {2010},
   volume                        = {21},
   number                        = {3},
   pages                         = {594--613},
   url                           = {https://www.doi.org/10.1287/ISRE.1100.0294},
   abstract                      = {This paper examines two important questions in the context of the social networking services (SNS) firms: what kind of competitive moves do SNS firms undertake and to what extent do the competitive moves impact firm performance? We blend the literature streams on information systems (IS) and strategic management and argue that given the unique characteristics of this nascent industry, SNS firms' competitive moves are likely to focus on value cocreation, as well as enhancement of the repertoire of their moves. We propose a conceptual model by blending value cocreation perspectives from the IS literature and repertoire of competitive actions from the competitive dynamics literature, and test our hypotheses using archival data. Results show that firms that emphasize value cocreation actions through the engagement of codevelopers in their technology platform and formation of strategic alliances enhance their performance. Furthermore, firms that undertake complex action repertoires achieve better performance. This study provides unique insights about the ways in which firms compete in the industry and has several implications for future research.},
   language                      = {en},
   cited_by                      = {72},
}

@article{GoesGuoLin2016,
   colrev_origin                 = {CROSSREF.bib/GoesGuoLin2016;DBLP.bib/000830;pdfs.bib/0000000827},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/27_3/GoesGuoLin2016.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2016.0635;;
                                    language:http://dx.doi.org/10.1287/ISRE.2016.0635;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.2016.0635;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|27|3|2016|goes-guo-lin|do-incentive-hierarchies-induce-user-effort-evidence-from-an-online-knowledge-exchange;},
   file                          = {pdfs/27_3/GoesGuoLin2016.pdf},
   doi                           = {10.1287/ISRE.2016.0635},
   dblp_key                      = {https://dblp.org/rec/journals/isr/Goes16},
   author                        = {Góes, Paulo B. and Guo, Chenhui and Lin, Mingfeng},
   journal                       = {Information Systems Research},
   title                         = {Do Incentive Hierarchies Induce User Effort? Evidence from an Online Knowledge Exchange},
   year                          = {2016},
   volume                        = {27},
   number                        = {3},
   pages                         = {497--516},
   url                           = {https://www.doi.org/10.1287/ISRE.2016.0635},
   abstract                      = {To motivate user contributions, user-generated content sites routinely deploy incentive hierarchies, where users achieve increasingly higher statuses in the community after achieving increasingly more difficult goals. Yet the existing empirical literature remains largely unclear whether such hierarchies are indeed effective in inducing user contributions. We gather data from a large online crowd-based knowledge exchange to answer this question, and draw on goal setting and status hierarchy theories to study users’ contributions before and after they reach consecutive ranks on a vertical incentive hierarchy. We find evidence that even though these glory-based incentives may motivate users to contribute more before the goals are reached, user contribution levels drop significantly after that. The positive effect on user contribution appears only temporary. Moreover, such impacts are increasingly smaller for higher ranks. Our results highlight some unintended and heretofore undocumented effects of incentive hierarchies, and have important implications for business models that rely on user contributions, such as knowledge exchange and crowdsourcing, as well as the broader phenomenon of gamification in other contexts.},
   language                      = {en},
   cited_by                      = {74},
}

@article{GoesKarugaTripathi2010,
   colrev_origin                 = {CROSSREF.bib/GoesKarugaTripathi2010;DBLP.bib/000500;pdfs.bib/0000000522},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/21_4/GoesKarugaTripathi2010.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.1080.0216;;
                                    language:http://dx.doi.org/10.1287/ISRE.1080.0216;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.1080.0216;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|21|4|2010|goes-karuga-tripathi|understanding-willingness-to-pay-formation-of-repeat-bidders-in-sequential-online-auctions;},
   file                          = {pdfs/21_4/GoesKarugaTripathi2010.pdf},
   doi                           = {10.1287/ISRE.1080.0216},
   dblp_key                      = {https://dblp.org/rec/journals/isr/GoesKT10},
   author                        = {Góes, Paulo B. and Karuga, Gilbert G. and Tripathi, Arvind K.},
   journal                       = {Information Systems Research},
   title                         = {Understanding Willingness-to-Pay Formation of Repeat Bidders in Sequential Online Auctions},
   year                          = {2010},
   volume                        = {21},
   number                        = {4},
   pages                         = {907--924},
   url                           = {https://www.doi.org/10.1287/ISRE.1080.0216},
   abstract                      = {A growing number of vendors are using a sequence of online auctions to sell large inventories of identical items. Although bidding strategies and bidder behavior in single auctions have been extensively studied, limited research exists on bidding in sequential auctions. We seek to explain how bidders in such an environment learn from the information, and form and update their willingness to pay (WTP). Using a large data set from an online auction retailer, we analyze the evolution of the bidders' WTP as well as the effect of auction design on bidders' WTP in sequential auctions. We see our study in the context of a longitudinal field experiment, in which we were able to track actions of repeat bidders over an extended period of time. Our results show that bidders' WTP in sequential auctions can be explained from their demand characteristics, their participation experience in previous auctions, outcomes in previous auctions, and auction design parameters. We also observe, characterize, and measure what we call a modified demand reduction effect exhibited across different auctions, over time, by multiunit demand bidders. Our findings are important to enable better auction mechanism design, and more sophisticated bidding tools that explore the rich information environment of sequential auctions.},
   language                      = {en},
   cited_by                      = {29},
}

@article{GoesLinYeung2014,
   colrev_origin                 = {CROSSREF.bib/GoesLinYeung2014;DBLP.bib/000727;pdfs.bib/0000000755},
