   colrev_id                     = {colrev_id1:|a|information-systems-research|21|4|2010|yoo-henfridsson-lyytinen|research-commentary-the-new-organizing-logic-of-digital-innovation-an-agenda-for-information-systems-research;
                                    colrev_id1:|a|information-systems-research|21|4|2010|yoo-henfridsson-lyytinen|research-commentarythe-new-organizing-logic-of-digital-innovation-an-agenda-for-information-systems-research;},
   file                          = {pdfs/21_4/YooHenfridssonLyytinen2010.pdf},
   doi                           = {10.1287/ISRE.1100.0322},
   dblp_key                      = {https://dblp.org/rec/journals/isr/YooHL10},
   author                        = {Yoo, Youngjin and Henfridsson, Ola and Lyytinen, Kalle},
   journal                       = {Information Systems Research},
   title                         = {Research Commentary - The New Organizing Logic of Digital Innovation - An Agenda for Information Systems Research},
   year                          = {2010},
   volume                        = {21},
   number                        = {4},
   pages                         = {724--735},
   url                           = {https://www.doi.org/10.1287/ISRE.1100.0322},
   abstract                      = {In this essay, we argue that pervasive digitization gives birth to a new type of product architecture: the layered modular architecture. The layered modular architecture extends the modular architecture of physical products by incorporating four loosely coupled layers of devices, networks, services, and contents created by digital technology. We posit that this new architecture instigates profound changes in the ways that firms organize for innovation in the future. We develop (1) a conceptual framework to describe the emerging organizing logic of digital innovation and (2) an information systems research agenda for digital strategy and the creation and management of corporate information technology infrastructures.},
   language                      = {en},
   cited_by                      = {978},
}

@article{YuanDasRameshEtAl2018,
   colrev_origin                 = {CROSSREF.bib/YuanDasRameshEtAl2018;DBLP.bib/000916;pdfs.bib/0000000340},
