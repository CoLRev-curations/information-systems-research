@article{KourandiKramerValletti2015,
   colrev_origin                 = {CROSSREF.bib/KourandiKramerValletti2015;DBLP.bib/000782;pdfs.bib/0000000266},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/26_2/KourandiKramerValletti2015.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2015.0567;;
