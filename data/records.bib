   colrev_id                     = {colrev_id1:|a|information-systems-research|31|2|2020|han-lappas-sabnis|the-importance-of-interactions-between-content-characteristics-and-creator-characteristics-for-studying-virality-in-social-media;},
   file                          = {pdfs/31_2/HanLappasSabnis2020.pdf},
   doi                           = {10.1287/ISRE.2019.0903},
   dblp_key                      = {https://dblp.org/rec/journals/isr/HanLS20},
   author                        = {Han, Yue and Lappas, Theodoros and Sabnis, Gaurav},
   journal                       = {Information Systems Research},
   title                         = {The Importance of Interactions Between Content Characteristics and Creator Characteristics for Studying Virality in Social Media},
   year                          = {2020},
   volume                        = {31},
   number                        = {2},
   pages                         = {576--588},
   url                           = {https://www.doi.org/10.1287/ISRE.2019.0903},
   abstract                      = {Why does a social media post go viral? Two approaches to understand this mystery are content-based research and creator-based research. Both content characteristics and creator characteristics have been examined for their influence on virality. But the relationships between them are rarely discussed. We propose an extension to our existing conceptual framework to study the interactions between content and creator variables. And we demonstrate the significance of the interactions using data from 800,000 tweets. We find that by adding content-–creator interactions, the predictive power of the model improves significantly, which underlines the importance of the interactions for studying virality in social media. We also provide insights for managers on shaping their social media presence and strategy to use social media popularity for marketing and brand building.},
   language                      = {en},
   cited_by                      = {14},
}

