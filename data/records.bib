   colrev_id                     = {colrev_id1:|a|information-systems-research|26|3|2015|krasnova-widjaja-buxmann-wenninger-benbasat|research-note-why-following-friends-can-hurt-you-an-exploratory-investigation-of-the-effects-of-envy-on-social-networking-sites-among-college-age-users;
                                    colrev_id1:|a|information-systems-research|26|3|2015|krasnova-widjaja-buxmann-wenninger-benbasat|research-notewhy-following-friends-can-hurt-you-an-exploratory-investigation-of-the-effects-of-envy-on-social-networking-sites-among-college-age-users;},
