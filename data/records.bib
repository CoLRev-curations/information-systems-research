   colrev_id                     = {colrev_id1:|a|information-systems-research|30|2|2019|wang-wang|effects-of-sponsorship-disclosure-on-perceived-integrity-of-biased-recommendation-agents-psychological-contract-violation-and-knowledge-based-trust-perspectives;},
   file                          = {pdfs/30_2/WangWang2019.pdf},
   doi                           = {10.1287/ISRE.2018.0811},
   dblp_key                      = {https://dblp.org/rec/journals/isr/WangW19},
   author                        = {Wang, Weiquan and Wang, May D.},
   journal                       = {Information Systems Research},
   title                         = {Effects of Sponsorship Disclosure on Perceived Integrity of Biased Recommendation Agents - Psychological Contract Violation and Knowledge-Based Trust Perspectives},
   year                          = {2019},
   volume                        = {30},
   number                        = {2},
   pages                         = {507--522},
   url                           = {https://www.doi.org/10.1287/ISRE.2018.0811},
   abstract                      = {Product recommendation agents (RAs) are widely employed by online merchants to facilitate consumers’ decision making. Users’ perceived integrity of these RAs becomes a critical trust concern when RAs apply sponsorship practices and recommend products biased toward sponsored products. Sponsorship disclosure is enforced by the U.S. Federal Trade Commission, but many technologies fail to comply probably because of their concerns on users’ trust in the biased technologies. This research investigate when sponsorship disclosure is most effective in enhancing users’ perceived RA integrity. A laboratory experiment revealed two major findings related to the benefits of sponsorship disclosure in enhancing users’ perceived integrity of a biased RA. First, for users with high prior knowledge about the prevalence of sponsorships used by RAs in general, sponsorship disclosure can reduce users’ perceived psychological contract violations of a biased RA and then increase users’ perceived RA integrity. For users with limited such prior knowledge, the disclosure fails to reduce these perceived violations. Second, regardless of the level of such prior knowledge of users, sponsorship disclosure enhances users’ perceived transparency of a biased RA, which, in turn, leads to perceived RA integrity.},
   language                      = {en},
   cited_by                      = {6},
}

@article{WangXiaoRao2015,
   colrev_origin                 = {CROSSREF.bib/WangXiaoRao2015;DBLP.bib/000750;pdfs.bib/0000000445},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/26_3/WangXiaoRao2015.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2015.0581;;
                                    language:http://dx.doi.org/10.1287/ISRE.2015.0581;;
                                    cited_by:http://dx.doi.org/10.1287/ISRE.2015.0581;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|26|3|2015|wang-xiao-rao|research-note-an-exploration-of-risk-characteristics-of-information-security-threats-and-related-public-information-search-behavior;
                                    colrev_id1:|a|information-systems-research|26|3|2015|wang-xiao-rao|research-notean-exploration-of-risk-characteristics-of-information-security-threats-and-related-public-information-search-behavior;},
   file                          = {pdfs/26_3/WangXiaoRao2015.pdf},
   doi                           = {10.1287/ISRE.2015.0581},
   dblp_key                      = {https://dblp.org/rec/journals/isr/WangXR15},
   author                        = {Wang, Jingguo and Xiao, Nan and Rao, H. Raghav},
   journal                       = {Information Systems Research},
   title                         = {Research Note - An Exploration of Risk Characteristics of Information Security Threats and Related Public Information Search Behavior},
   year                          = {2015},
   volume                        = {26},
   number                        = {3},
   pages                         = {619--633},
   url                           = {https://www.doi.org/10.1287/ISRE.2015.0581},
   abstract                      = {Information security (IS) threats are increasingly pervasive, and search engines are being used by the public as the primary tool for searching for relevant information. This research investigates the following two questions: (1) How can different IS threats be characterized and distinguished in terms of their risk characteristics? and (2) how are risk characteristics related to public searches for information on IS threats? Applying psychometric analysis, our analyses of survey data first show that unknown risk and dread risk are two underlying dimensions that can characterize different IS threats. Drawing broadly on the literature of information foraging theory, we examine the influence of risk characteristics on public searches for information on these threats. We utilize a search engine log to extract searches related to IS threats. We develop and estimate a system of equations with correlated individual-specific error terms using the Markov Chain Monte Carlo method. We find that the two risk characteristics exert differential impacts on information search behavior (including types of information sought, number of pages viewed, and length of query). The implications for IS research and practice are discussed.},
   language                      = {en},
   cited_by                      = {12},
}

@article{WangYanZhouEtAl2020,
   colrev_origin                 = {CROSSREF.bib/WangYanZhouEtAl2020;DBLP.bib/001166;pdfs.bib/0000000288},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/31_2/WangYanZhouEtAl2020.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2019.0901;;
                                    language:http://dx.doi.org/10.1287/ISRE.2019.0901;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.2019.0901;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|31|2|2020|wang-yan-zhou-guo-heim|understanding-physicians-online-offline-behavior-dynamics-an-empirical-study;},
   file                          = {pdfs/31_2/WangYanZhouEtAl2020.pdf},
   doi                           = {10.1287/ISRE.2019.0901},
   dblp_key                      = {https://dblp.org/rec/journals/isr/WangYZGH20},
   author                        = {Wang, Liuan and Yan, Lu Lucy and Zhou, Tongxin and Guo, Xitong and Heim, Gregory R.},
   journal                       = {Information Systems Research},
   title                         = {Understanding Physicians' Online-Offline Behavior Dynamics - An Empirical Study},
   year                          = {2020},
   volume                        = {31},
   number                        = {2},
   pages                         = {537--555},
   url                           = {https://www.doi.org/10.1287/ISRE.2019.0901},
   abstract                      = {Online healthcare platforms allow physicians and patients to communicate in a timely manner. Yet little is known about how physicians’ online and offline activities affect each other and, consequently, the healthcare system. We collected data from both online and offline channels to study physicians’ online-offline behavior dynamics. We find that physicians’ online activities can lead to a higher service quantity in offline channels, whereas offline activities may reduce physicians’ online services because of resource constraints. We also find that the more offline patients that physicians serve, the more articles the physicians will likely share in online healthcare platforms. These findings are of great importance to practitioners and policy makers. Our work provides evidence that online healthcare platforms supplement offline services and thus lessen the concern that physicians’ participation in online healthcare platforms will negatively influence offline healthcare services. Our findings also indicate the need for the improvement of online-offline coordination and better system design.},
   language                      = {en},
   cited_by                      = {14},
}

@article{WangZhangHann2018,
