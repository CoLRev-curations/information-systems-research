   colrev_id                     = {colrev_id1:|a|information-systems-research|27|4|2016|faraj-von-krogh-monteiro-lakhani|special-section-introduction-online-community-as-space-for-knowledge-flows;
                                    colrev_id1:|a|information-systems-research|27|4|2016|faraj-von-krogh-monteiro-lakhani|special-section-introductiononline-community-as-space-for-knowledge-flows;},
