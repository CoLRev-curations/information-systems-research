   file                          = {pdfs/28_3/GomezSalazarVargas2017.pdf},
   doi                           = {10.1287/ISRE.2017.0705},
   dblp_key                      = {https://dblp.org/rec/journals/isr/GomezSV17},
   author                        = {Gómez, Jaime and Salazar, Idana and Vargas, Pilar},
   journal                       = {Information Systems Research},
   title                         = {Does Information Technology Improve Open Innovation Performance? An Examination of Manufacturers in Spain},
   year                          = {2017},
   volume                        = {28},
   number                        = {3},
   pages                         = {661--675},
   url                           = {https://www.doi.org/10.1287/ISRE.2017.0705},
   abstract                      = {Our objective in this paper is to explore the impact of information technology (IT) on the performance of open innovation models. We study the effect of IT on the production of patents and product innovations. Our analysis of a large panel of Spanish manufacturing firms confirms that the effect of the proportion of external research and development (R&D) investments on the production of patents and new products has an inverted U-shape. The results also reveal that investments in IT positively moderate the consequences of open innovation on innovation performance by reducing identification, assimilation, and utilization costs. Our results provide evidence of the viability and benefit of a new strategic alternative to innovation that is different than the traditional process of vertically integrated R&D. The online appendix is available at https://doi.org/10.1287/isre.2017.0705 .},
   language                      = {en},
   cited_by                      = {31},
}

