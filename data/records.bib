                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.2019.0874;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|31|1|2020|sun-fan-tan|an-empirical-analysis-of-seller-advertising-strategies-in-an-online-marketplace;},
   file                          = {pdfs/31_1/SunFanTan2020.pdf},
   doi                           = {10.1287/ISRE.2019.0874},
   dblp_key                      = {https://dblp.org/rec/journals/isr/SunFT20},
   author                        = {Sun, Haoyan and Fan, Ming and Tan, Yong},
   journal                       = {Information Systems Research},
   title                         = {An Empirical Analysis of Seller Advertising Strategies in an Online Marketplace},
   year                          = {2020},
   volume                        = {31},
   number                        = {1},
   pages                         = {37--56},
   url                           = {https://www.doi.org/10.1287/ISRE.2019.0874},
   abstract                      = {Online marketplaces, such as Taobao, Amazon, and eBay, are increasingly adopting innovative business models such as paid advertising as an important revenue source. We study the effectiveness of two popular advertising tools, sponsored search and social media endorsement, in increasing online traffic and sales for online sellers at a retail e-commerce platform. We find that both sponsored search and social media endorsement can significantly increase traffic for sellers, with sponsored search being more effective than social media endorsement. However, only sponsored research has a positive and significant impact on sales. The two strategies also have differential effect on sellers with low and high reputations. Our findings are of great importance, as they provide additional insights into how these advertising tools can work more effectively in increasing traffic and sales for sellers with different reputation levels, thus providing important implications to guide the strategic choices for sellers as well as the platform.},
   language                      = {en},
   cited_by                      = {18},
}

@article{SunFangLimEtAl2012,
   colrev_origin                 = {CROSSREF.bib/SunFangLimEtAl2012;DBLP.bib/000588;pdfs.bib/0000000763},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/23_4/SunFangLimEtAl2012.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.1120.0421;;
                                    language:http://dx.doi.org/10.1287/ISRE.1120.0421;;
                                    cited_by:http://dx.doi.org/10.1287/ISRE.1120.0421;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|23|4|2012|sun-fang-lim-straub|user-satisfaction-with-information-technology-service-delivery-a-social-capital-perspective;},
   file                          = {pdfs/23_4/SunFangLimEtAl2012.pdf},
   doi                           = {10.1287/ISRE.1120.0421},
   dblp_key                      = {https://dblp.org/rec/journals/isr/SunFLS12},
   author                        = {Sun, Yongqiang and Fang, Yulin and Lim, Kai H. and Straub, Detmar W.},
   journal                       = {Information Systems Research},
   title                         = {User Satisfaction with Information Technology Service Delivery - A Social Capital Perspective},
   year                          = {2012},
   volume                        = {23},
   number                        = {4},
   pages                         = {1195--1211},
   url                           = {https://www.doi.org/10.1287/ISRE.1120.0421},
   abstract                      = {Existing research has long considered service quality as a primary determinant of user satisfaction with information technology (IT) service delivery. In response to the knowledge-intensive and collaborative nature of IT service delivery in the contemporary business context, we advance the theoretical understanding of user satisfaction by re-conceptualizing IT service delivery as a bilateral, relational process between the IT staff and users. Based on this reconceptualization, we draw on social capital theory to examine the antecedents of user satisfaction with IT service delivery. Specifically, we posit that two major dimensions of social capital, i.e., cognitive capital and relational capital, not only positively affect user satisfaction but also strengthen the established relationship between service quality and user satisfaction. Furthermore, we propose that the effect of the other dimension of social capital—structural capital—on user satisfaction is fully mediated through cognitive capital and relational capital. A field study of 159 users in four financial companies provides general empirical support for our hypotheses. Theoretical and practical implications of these findings are discussed.},
   language                      = {en},
   cited_by                      = {102},
}

@article{SunLuRui2020,
   colrev_origin                 = {CROSSREF.bib/SunLuRui2020;DBLP.bib/001162;pdfs.bib/0000000793},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/31_3/SunLuRui2020.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2020.0926;;
                                    language:http://dx.doi.org/10.1287/ISRE.2020.0926;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.2020.0926;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|31|3|2020|sun-lu-rui|does-telemedicine-reduce-emergency-room-congestion-evidence-from-new-york-state;},
   file                          = {pdfs/31_3/SunLuRui2020.pdf},
   doi                           = {10.1287/ISRE.2020.0926},
   dblp_key                      = {https://dblp.org/rec/journals/isr/SunLR20},
   author                        = {Sun, Shujing and Lu, Susan Feng and Rui, Huaxia},
   journal                       = {Information Systems Research},
   title                         = {Does Telemedicine Reduce Emergency Room Congestion? Evidence from New York State},
   year                          = {2020},
   volume                        = {31},
   number                        = {3},
   pages                         = {972--986},
   url                           = {https://www.doi.org/10.1287/ISRE.2020.0926},
   abstract                      = {Overcrowding in emergency rooms (ERs) is a common yet nagging problem. It not only is costly for hospitals but also compromises care quality and patient experience. Our paper provides solid evidence that telemedicine can significantly improve ER care delivery, especially in the presence of demand and supply fluctuations. We believe such findings are critical for ERs, due to the special setting of unscheduled arrivals leading to high unpredictability of patient traffic. Additional evidence suggests that the efficiency gained from telemedicine does not come at the expense of lower care quality or higher medical expenditure, which points to telemedicine as a feasible solution to the ER overcrowding problem. For healthcare practitioners, our paper highlights the general applicability of telemedicine through the “hub and spoke” architecture. Besides increasing patients’ access to more immediate care from specialists who were not available otherwise, telemedicine enables flexible resource allocation for any hospitals, regardless of where hospitals are located. Our research also provides ground for policymakers to incentivize hospitals to adopt telemedicine in ER, which we believe is critical given the relatively low adoption rate, the lack of direct evidence on its effectiveness, and the current inflexibility of reimbursement policies regarding the application of ER telemedicine.},
   language                      = {en},
   cited_by                      = {22},
}

