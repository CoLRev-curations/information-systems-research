@article{NaultZimmermann2019,
   colrev_origin                 = {CROSSREF.bib/NaultZimmermann2019;DBLP.bib/001010;pdfs.bib/0000000829},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/30_3/NaultZimmermann2019.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2018.0828;;
                                    language:http://dx.doi.org/10.1287/ISRE.2018.0828;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.2018.0828;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|30|3|2019|nault-zimmermann|balancing-openness-and-prioritization-in-a-two-tier-internet;},
   file                          = {pdfs/30_3/NaultZimmermann2019.pdf},
   doi                           = {10.1287/ISRE.2018.0828},
   dblp_key                      = {https://dblp.org/rec/journals/isr/NaultZ19},
   author                        = {Nault, Barrie R. and Zimmermann, Steffen},
   journal                       = {Information Systems Research},
   title                         = {Balancing Openness and Prioritization in a Two-Tier Internet},
   year                          = {2019},
   volume                        = {30},
   number                        = {3},
   pages                         = {745--763},
   url                           = {https://www.doi.org/10.1287/ISRE.2018.0828},
   abstract                      = {The open internet is plagued by congestion that restricts the development of sophisticated internet-based services. Broadband and edge providers have proposed a two-tier internet with a fee-based fast lane that coexists with the open internet. This requires a restriction of internet openness, also known as network neutrality, in the fast-lane internet. Opponents of a two-tier internet believe it would hinder innovation and cause underinvestment in the open internet. The challenge is for policy to balance a fee-based fast lane with the viability of the open internet. We find that edge providers with greater bandwidth requirements per unit of output convert to the fast lane and that the fast lane can drive innovation from edge providers with high bandwidth requirements. The broadband provider chooses fixed fee pricing for the fast lane but has no incentive to increase internet capacity as long as the open internet is not monetized. With no investments in internet capacity, all edge providers of the open Internet and their end users are worse off with a two-tier internet. To maintain quality-of-service in the open internet and to increase social welfare, a two-tier internet has to be coupled with policy whereby a portion of broadband provider profit is invested in internet capacity.},
   language                      = {en},
   cited_by                      = {0},
}

@article{NevoNevoPinsonneault2021,
   colrev_origin                 = {CROSSREF.bib/NevoNevoPinsonneault2021;DBLP.bib/001095;pdfs.bib/0000000200},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
