                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.2015.0573;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|26|2|2015|tiwana|evolutionary-competition-in-platform-ecosystems;},
   file                          = {pdfs/26_2/Tiwana2015.pdf},
