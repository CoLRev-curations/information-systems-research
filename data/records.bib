   colrev_id                     = {colrev_id1:|a|information-systems-research|23|1|2012|kang-lim-kim-yang|research-note-a-multilevel-analysis-of-the-effect-of-group-appropriation-on-collaborative-technologies-use-and-performance;
                                    colrev_id1:|a|information-systems-research|23|1|2012|kang-lim-kim-yang|research-notea-multilevel-analysis-of-the-effect-of-group-appropriation-on-collaborative-technologies-use-and-performance;},
