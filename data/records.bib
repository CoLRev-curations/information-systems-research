   colrev_data_provenance        = {file:pdfs/23_2/ZhuZhou2012.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.1110.0358;;
                                    language:http://dx.doi.org/10.1287/ISRE.1110.0358;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.1110.0358;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|23|2|2012|zhu-zhou|research-note-lock-in-strategy-in-software-competition-open-source-software-vs-proprietary-software;
                                    colrev_id1:|a|information-systems-research|23|2|2012|zhu-zhou|research-notelock-in-strategy-in-software-competition-open-source-software-vs-proprietary-software;},
   file                          = {pdfs/23_2/ZhuZhou2012.pdf},
   doi                           = {10.1287/ISRE.1110.0358},
   dblp_key                      = {https://dblp.org/rec/journals/isr/ZhuZ12},
   author                        = {Zhu, Kevin Xiaoguo and Zhou, Zach Zhizhong},
   journal                       = {Information Systems Research},
   title                         = {Research Note - Lock-In Strategy in Software Competition - Open-Source Software vs. Proprietary Software},
   year                          = {2012},
   volume                        = {23},
   number                        = {2},
   pages                         = {536--545},
   url                           = {https://www.doi.org/10.1287/ISRE.1110.0358},
   abstract                      = {Open-source software poses a serious challenge to proprietary software vendors. “Lock in customers” seems a tempting strategy for proprietary software vendors, who attempt to lock in customers by creating switching costs. This paper examines whether such a lock-in strategy will indeed benefit proprietary software vendors facing competition from open-source software, who can credibly commit future prices. Developing a two-period duopoly model in which software products are differentiated and customers are heterogeneous, we find that the lock-in strategy is actually counterproductive in competing against open-source software. In fact, giving customers the freedom of choice may end up benefiting the proprietary software vendor. In terms of the broader effect, we find that lock-in reduces overall social welfare, but certain customers may actually be better off with it. Finally, we show that the lock-in strategy works differently for different types of customers in the software market (i.e., foresighted versus myopic customers). This suggests that customer behavior could significantly alter the equilibrium strategy of software vendors.},
   language                      = {en},
   cited_by                      = {35},
}

@article{ZhuangFangLeeEtAl2021,
   colrev_origin                 = {CROSSREF.bib/ZhuangFangLeeEtAl2021;DBLP.bib/001116;pdfs.bib/0000000390},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/32_4/ZhuangFangLeeEtAl2021.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2021.1039;;
                                    language:http://dx.doi.org/10.1287/ISRE.2021.1039;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.2021.1039;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|32|4|2021|zhuang-fang-lee-li|the-effects-of-price-rank-on-clicks-and-conversions-in-product-list-advertising-on-online-retail-platforms;},
   file                          = {pdfs/32_4/ZhuangFangLeeEtAl2021.pdf},
   doi                           = {10.1287/ISRE.2021.1039},
   dblp_key                      = {https://dblp.org/rec/journals/isr/ZhuangFLL21},
   author                        = {Zhuang, Mengzhou and Fang, Eric and Lee, Jongkuk and Li, Xiaoling},
   journal                       = {Information Systems Research},
   title                         = {The Effects of Price Rank on Clicks and Conversions in Product List Advertising on Online Retail Platforms},
   year                          = {2021},
   volume                        = {32},
   number                        = {4},
   pages                         = {1412--1430},
   url                           = {https://www.doi.org/10.1287/ISRE.2021.1039},
   abstract                      = {In light of the critical role of price information in consumers’ decision making, this study investigates the effect of price rank on consumers’ responses to product list advertising (PLA). The research documents that the price rank is more influential than actual price for PLA. In addition, the research highlights a tradeoff in price-rank decisions: A price rank that drives more clicks does not necessarily lead to more conversions; to drive traffic, managers should strive for an extreme (i.e., either high or low) to elicit more clicks, then follow up with online engagement tools (e.g., cross-selling and product recommendations). To maximize direct revenue, managers instead should strive for moderate ranks to satisfy consumers’ desire for a compromise between price and quality. However, consumers without uncertainty tend to rely less on price rank, so the effects diminish among specific keywords and increase among popular keywords. In order to achieve the desired price ranks, firms participating in PLA might monitor and adjust their advertising offers. There are commonly two specific avenues: Change the product price if the required change is within a certain range or change the advertised product if the required price change is beyond a certain range.},
   language                      = {en},
   cited_by                      = {1},
}

@article{ZimmermannHerrmannKundischEtAl2018,
   colrev_origin                 = {CROSSREF.bib/ZimmermannHerrmannKundischEtAl2018;DBLP.bib/000951;pdfs.bib/0000001075},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/29_4/ZimmermannHerrmannKundischEtAl2018.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2017.0764;;
                                    language:http://dx.doi.org/10.1287/ISRE.2017.0764;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.2017.0764;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|29|4|2018|zimmermann-herrmann-kundisch-nault|decomposing-the-variance-of-consumer-ratings-and-the-impact-on-price-and-demand;},
   file                          = {pdfs/29_4/ZimmermannHerrmannKundischEtAl2018.pdf},
   doi                           = {10.1287/ISRE.2017.0764},
   dblp_key                      = {https://dblp.org/rec/journals/isr/ZimmermannHKN18},
   author                        = {Zimmermann, Steffen and Herrmann, Philipp and Kundisch, Dennis and Nault, Barrie R.},
   journal                       = {Information Systems Research},
   title                         = {Decomposing the Variance of Consumer Ratings and the Impact on Price and Demand},
   year                          = {2018},
   volume                        = {29},
   number                        = {4},
   pages                         = {984--1002},
   url                           = {https://www.doi.org/10.1287/ISRE.2017.0764},
   abstract                      = {Consumer ratings play a decisive role in purchases by online shoppers. Although the effects of the average and the number of consumer ratings on future product pricing and demand have been studied with some conclusive results, the effects of the variance of these ratings are less well understood. We develop a model where we decompose the variance of consumer ratings into two sources: taste differences about search and experience attributes of a durable good, and quality differences among instances of this good in the form of product failure. We find that (i) optimal price increases and demand decreases in variance caused by taste differences, (ii) optimal price and demand decrease in variance caused by quality differences, and (iii) when holding the average rating as well as the total variance constant, for products with low total variance, both price and demand increase in the relative share of variance caused by taste differences. Counter to intuition, we demonstrate that risk-averse consumers may prefer a higher-priced product with a higher variance in ratings when deciding between two similar products with the same average rating.},
   language                      = {en},
   cited_by                      = {18},
}

