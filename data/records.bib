@article{BrynjolfssonHuSmith2010,
   colrev_origin                 = {CROSSREF.bib/BrynjolfssonHuSmith2010;DBLP.bib/000467;pdfs.bib/0000001088},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/21_4/BrynjolfssonHuSmith2010.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.1100.0325;;
                                    language:http://dx.doi.org/10.1287/ISRE.1100.0325;;
                                    cited_by:http://dx.doi.org/10.1287/ISRE.1100.0325;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|21|4|2010|brynjolfsson-hu-smith|research-commentary-long-tails-vs-superstars-the-effect-of-information-technology-on-product-variety-and-sales-concentration-patterns;},
   file                          = {pdfs/21_4/BrynjolfssonHuSmith2010.pdf},
   doi                           = {10.1287/ISRE.1100.0325},
   dblp_key                      = {https://dblp.org/rec/journals/isr/BrynjolfssonHS10},
   author                        = {Brynjolfsson, Erik and Hu, Yu and Smith, Michael D.},
   journal                       = {Information Systems Research},
   title                         = {Research Commentary - Long Tails vs. Superstars - The Effect of Information Technology on Product Variety and Sales Concentration Patterns},
   year                          = {2010},
   volume                        = {21},
   number                        = {4},
   pages                         = {736--747},
   url                           = {https://www.doi.org/10.1287/ISRE.1100.0325},
   abstract                      = {The Internet and related information technologies are transforming the distribution of product sales across products, and these effects are likely to grow in coming years. Both the Long Tail and the Superstar effect are manifestations of these changes, yet researchers lack consistent metrics or models for integrating and extending their insights and predictions. In this paper, we begin with a taxonomy of the technological and nontechnological drivers of both Long Tails and Superstars and then define and compare the key metrics for analyzing these phenomena. The core of the paper describes a large and promising set of questions forming a research agenda. Important opportunities exist for understanding future changes in sales concentration patterns; the impact on supply chains (including cross-channel competition, competition within the Internet channel, implications for the growth of firms, and the balance of power within the supply chain); implications for pricing, promotion, and product design; and, ultimately, the potential effects on society in general. Our approach provides an introduction to some of the relevant research findings and allows us to identify opportunities for cross-pollination of methods and insights from related research topics.},
   language                      = {en},
   cited_by                      = {111},
}

@article{BuckmanBockstedtHashim2019,
   colrev_origin                 = {CROSSREF.bib/BuckmanBockstedtHashim2019;DBLP.bib/000985;pdfs.bib/0000000782},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/30_2/BuckmanBockstedtHashim2019.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2018.0818;;
                                    language:http://dx.doi.org/10.1287/ISRE.2018.0818;;
                                    cited_by:http://dx.doi.org/10.1287/ISRE.2018.0818;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|30|2|2019|buckman-bockstedt-hashim|relative-privacy-valuations-under-varying-disclosure-characteristics;},
   file                          = {pdfs/30_2/BuckmanBockstedtHashim2019.pdf},
   doi                           = {10.1287/ISRE.2018.0818},
   dblp_key                      = {https://dblp.org/rec/journals/isr/BuckmanBH19},
   author                        = {Buckman, Joseph R. and Bockstedt, Jesse C. and Hashim, Matthew J.},
   journal                       = {Information Systems Research},
   title                         = {Relative Privacy Valuations Under Varying Disclosure Characteristics},
   year                          = {2019},
   volume                        = {30},
   number                        = {2},
   pages                         = {375--388},
   url                           = {https://www.doi.org/10.1287/ISRE.2018.0818},
   abstract                      = {We investigate changes to the value that individuals place on the online disclosure of their private information in the presence of multiple privacy factors. We capture individuals’ willingness-to-accept for a privacy disclosure in a series of randomized experiments that manipulate characteristics of a required privacy disclosure by altering the information context, the intended secondary use of the disclosed private information, and the requirement to disclose personally identifying information. We collect data from two populations (college students and Amazon Mechanical Turk workers) to aid with generalizability of our results. Across the experiments, we consistently observe null effects for each of the privacy factors. The results provide a unique perspective on privacy valuations by showing that results from prior research on simple privacy decisions may not translate to more realistic, complex privacy disclosure decisions that involve multiple factors. Our findings suggest that disclosing private information may be an all or nothing type of decision as opposed to an activation of individual factors proposed by prior literature as important in a multidimension private information disclosure. This study provides managerial insight into the possible evolution of online disclosure decisions, especially in settings that incorporate multiple disclosure dimensions.},
   language                      = {en},
   cited_by                      = {8},
}

@article{BurgessGrimshawShaw2017,
