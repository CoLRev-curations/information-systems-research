@article{Unknown2011e,
   colrev_origin                 = {CROSSREF.bib/000258},
   colrev_status                 = {rev_prescreen_excluded},
   colrev_masterdata_provenance  = {journal:https://api.crossref.org/works/10.1287/ISRE.1110.0375;;
                                    title:https://api.crossref.org/works/10.1287/ISRE.1110.0375;;
                                    year:https://api.crossref.org/works/10.1287/ISRE.1110.0375;;
                                    volume:https://api.crossref.org/works/10.1287/ISRE.1110.0375;;
                                    number:https://api.crossref.org/works/10.1287/ISRE.1110.0375;;},
   colrev_data_provenance        = {doi:https://api.crossref.org/works/10.1287/ISRE.1110.0375;;
                                    abstract:https://api.crossref.org/works/10.1287/ISRE.1110.0375;;
                                    language:https://api.crossref.org/works/10.1287/ISRE.1110.0375;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.1110.0375;;
                                    url:https://api.crossref.org/works/10.1287/ISRE.1110.0375;;},
