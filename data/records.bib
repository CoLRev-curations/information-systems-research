@article{KrishnanPetersPadmanEtAl2005,
   colrev_origin                 = {CROSSREF.bib/KrishnanPetersPadmanEtAl2005;DBLP.bib/000313;pdfs.bib/0000000117},
   colrev_status                 = {pdf_imported},
   colrev_masterdata_provenance  = {CURATED:https://github.com/geritwagner/information-systems-research;;},
   colrev_data_provenance        = {file:pdfs/16_3/KrishnanPetersPadmanEtAl2005.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.1050.0063;;
