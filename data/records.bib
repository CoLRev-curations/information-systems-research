   colrev_masterdata_provenance  = {journal:https://api.crossref.org/works/10.1287/ISRE.1110.0417;;
                                    title:https://api.crossref.org/works/10.1287/ISRE.1110.0417;;
                                    year:https://api.crossref.org/works/10.1287/ISRE.1110.0417;;
                                    volume:https://api.crossref.org/works/10.1287/ISRE.1110.0417;;
                                    number:https://api.crossref.org/works/10.1287/ISRE.1110.0417;;},
   colrev_data_provenance        = {doi:https://api.crossref.org/works/10.1287/ISRE.1110.0417;;
                                    abstract:https://api.crossref.org/works/10.1287/ISRE.1110.0417;;
                                    language:https://api.crossref.org/works/10.1287/ISRE.1110.0417;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.1110.0417;;
                                    url:https://api.crossref.org/works/10.1287/ISRE.1110.0417;;},
