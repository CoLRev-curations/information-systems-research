   colrev_data_provenance        = {file:pdfs/27_1/PannielloGorgoglioneTuzhilin2016.pdf;;
                                    abstract:http://dx.doi.org/10.1287/ISRE.2015.0610;;
                                    language:http://dx.doi.org/10.1287/ISRE.2015.0610;;
                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.2015.0610;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|27|1|2016|panniello-gorgoglione-tuzhilin|research-note-in-carss-we-trust-how-context-aware-recommendations-affect-customers-trust-and-other-business-performance-measures-of-recommender-systems;
                                    colrev_id1:|a|information-systems-research|27|1|2016|panniello-gorgoglione-tuzhilin|research-notein-carss-we-trust-how-context-aware-recommendations-affect-customers-trust-and-other-business-performance-measures-of-recommender-systems;},
   file                          = {pdfs/27_1/PannielloGorgoglioneTuzhilin2016.pdf},
   doi                           = {10.1287/ISRE.2015.0610},
   dblp_key                      = {https://dblp.org/rec/journals/isr/PannielloGT16},
   author                        = {Panniello, Umberto and Gorgoglione, Michele and Tuzhilin, Alexander},
   journal                       = {Information Systems Research},
   title                         = {Research Note - In CARSs We Trust - How Context-Aware Recommendations Affect Customers' Trust and Other Business Performance Measures of Recommender Systems},
   year                          = {2016},
   volume                        = {27},
   number                        = {1},
   pages                         = {182--196},
   url                           = {https://www.doi.org/10.1287/ISRE.2015.0610},
   abstract                      = {Most of the work on context-aware recommender systems has focused on demonstrating that the contextual information leads to more accurate recommendations. Little work has been done, however, on studying how much the contextual information affects the business performance. In this paper, we study how including context in recommendations affects customers’ trust, sales, and other crucial business-related performance measures. To do this, we delivered content-based and context-aware recommendations through a live controlled experiment with real customers of a commercial European online publisher. We measured the recommendations’ accuracy and diversification, how much customers spent purchasing products during the experiment, the quantity and price of their purchases, and the customers’ level of trust. We show that collecting and using contextual information in recommendations affects business-related performance measures, such as company sales, by improving the accuracy and diversification of recommendations, which in turn improves trust and, ultimately, business performance results.},
   language                      = {en},
   cited_by                      = {42},
}

@article{PantSheng2015,
   colrev_origin                 = {CROSSREF.bib/PantSheng2015;DBLP.bib/000786;pdfs.bib/0000000014},
