                                    cited_by:https://api.crossref.org/works/10.1287/ISRE.2019.0847;;},
   colrev_id                     = {colrev_id1:|a|information-systems-research|30|3|2019|abbasi-li-adjeroh-abate-zheng|dont-mention-it-analyzing-user-generated-content-signals-for-early-adverse-event-warnings;},
   file                          = {pdfs/30_3/AbbasiLiAdjerohEtAl2019.pdf},
   doi                           = {10.1287/ISRE.2019.0847},
   dblp_key                      = {https://dblp.org/rec/journals/isr/AbbasiLAAZ19},
   author                        = {Abbasi, Ahmed and Li, Jingjing and Adjeroh, Donald A. and Abate, Marie and Zheng, Wanhong},
   journal                       = {Information Systems Research},
   title                         = {Don't Mention It? Analyzing User-Generated Content Signals for Early Adverse Event Warnings},
   year                          = {2019},
   volume                        = {30},
   number                        = {3},
   pages                         = {1007--1028},
   url                           = {https://pubsonline.informs.org/doi/10.1287/isre.2019.0847},
   abstract                      = {With greater impetus on broad postmarket surveillance, the Voice of the Customer (VoC) has emerged as an important source of information for understanding consumer experiences and identifying potential issues. In organizations, risk management groups are increasingly interested in working with their information technology teams to develop robust VoC listening platforms. Two key challenges have impeded success. First, prior work has leveraged diverse sets of channels, adverse event types, and modeling methods, resulting in diverging conclusions regarding the viability and efficacy of various user-generated channels and accompanying modeling methods. Second, many existing detection methods rely on “mention models” that have low detection rates, have high false positives, and lack timeliness.},
   language                      = {en},
   cited_by                      = {14},
}

@article{AbhishekGuajardoZhang2021,
   colrev_origin                 = {CROSSREF.bib/AbhishekGuajardoZhang2021;DBLP.bib/001060;pdfs.bib/0000000641},
