   colrev_id                     = {colrev_id1:|a|information-systems-research|28|4|2017|liang-wang-xue-ge|unraveling-the-alignment-paradox-how-does-business-it-alignment-shape-organizational-agility;
                                    colrev_id1:|a|information-systems-research|28|4|2017|liang-wang-xue-ge|unraveling-the-alignment-paradox-how-does-businessit-alignment-shape-organizational-agility;},
